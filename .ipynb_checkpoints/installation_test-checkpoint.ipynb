    "# import TensorFlow\n",
    "import tensorflow as tf\n",
    "\n",
    "sess = tf.Session()\n",
    "\n",
    "# Verify we can print a string\n",
    "hello = tf.constant(\"Hello TensorFlow\")\n",
    "print(sess.run(hello))\n",
    "\n",
    "# Perform some simple math\n",
    "a = tf.constant(20)\n",
    "b = tf.constant(22)\n",
    "print('a + b = {0}'.format(sess.run(a + b)))"
